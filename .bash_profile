# RC
[[ -f ~/.bashrc ]] && . ~/.bashrc

# Prompt
[[ -f ~/.bash_prompt ]] && . ~/.bash_prompt

# Aliases
[[ -f ~/.bash_aliases ]] && . ~/.bash_aliases

# Functions
[[ -f ~/.bash_aliases ]] && . ~/.bash_functions

# Homebrew
[[ -f /usr/local/etc/bash_completion ]] && . /usr/local/etc/bash_completion

